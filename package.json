{
  "name": "react-input-pin-code",
  "version": "2.0.0",
  "description": "Pin input built with React component and styled-components",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/luffy84217/react-input-pin-code.git"
  },
  "license": "MIT",
  "homepage": "https://luffy84217.github.io/react-input-pin-code/",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "default": "./dist/components/*/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./styles/*": "./dist/styles/*"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "prepack": "npm run build",
    "build": "rollup --config rollup.config.js --configPlugin rollup-plugin-typescript2",
    "lint": "tsc --noEmit",
    "test": "jest --config jest.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages",
    "validate": "npm run lint && npm run build && npm test -- --coverage",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prerelease": "npm run validate",
    "version:patch": "commit-and-tag-version --release-as patch",
    "version:minor": "commit-and-tag-version --release-as minor",
    "version:major": "commit-and-tag-version --release-as major"
  },
  "keywords": [
    "react",
    "react-components",
    "component",
    "input",
    "pin",
    "code",
    "chiffre",
    "field",
    "sipher",
    "pin-code"
  ],
  "author": {
    "name": "Asriel Yu",
    "email": "luffy8421@gmail.com"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/prompt": "^18.4.3",
    "@eslint/js": "^9.32.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/react-vite": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@swc/core": "^1.3.101",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-test-renderer": "^18.3.1",
    "@types/string-hash": "^1.1.3",
    "browserslist": "^4.22.2",
    "circular-dependency-plugin": "^5.2.2",
    "commit-and-tag-version": "^12.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dts-bundle-generator": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "^0.8.0",
    "glob": "^10.3.10",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.0",
    "pinst": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^4.45.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.69.5",
    "storybook": "^8.1.6",
    "string-hash": "^1.1.3",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "4.2",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-scss": "^5.3.2",
    "the-new-css-reset": "^1.11.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.38.0",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "peerDependencies": {
    "clsx": ">=2",
    "react": ">=18",
    "react-dom": ">=18",
    "the-new-css-reset": ">=1"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=7"
  },
  "files": [
    "dist"
  ]
}
