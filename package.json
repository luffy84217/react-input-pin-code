{
  "name": "react-input-pin-code",
  "version": "0.3.1",
  "description": "Pin input built with React component and styled-components",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "rollup --config",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "storybook": "start-storybook -p 6006",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prerelease": "npm run validate",
    "release": "standard-version",
    "postrelease": "git push && git push --follow-tags && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "validate": "npm run typecheck && npm run build && npm run lint && npm test"
  },
  "keywords": [
    "react",
    "react-components",
    "styled-components",
    "component",
    "input",
    "pin",
    "code",
    "chiffre",
    "field",
    "sipher",
    "pin-code"
  ],
  "author": {
    "name": "Asriel Yu",
    "email": "luffy8421@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/react": "^6.3.11",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.27",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.3",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11.2.3",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-version": "^9.3.2",
    "styled-components": "^5.3.3",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0",
    "prop-types": "^15.7.2",
    "styled-components": ">= 4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
